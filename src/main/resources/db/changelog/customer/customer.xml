<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">
    <changeSet id="customer" author="markush">
        <createTable tableName="customer">
            <column name="id" type="bigserial"> <constraints nullable="false" /></column>
            <column name="name" type="varchar(255)"></column>
        </createTable>
    </changeSet>
    <changeSet id="customer.pk" author="markush">
        <addPrimaryKey tableName="customer" columnNames="id" constraintName="customer.pk"/>
    </changeSet>
</databaseChangeLog>